@model List<object[]>

<h2 class="text-center">Round Matches</h2><br />
<table class="table table-hover table-responsive table-striped table-condensed">
    <tbody>
        @{
            foreach (var i in Model)
            {
                <tr class="row">
                    <td class="col-lg-2 text-center">@i[0] </td>
                    <td class="col-lg-1 text-center">@i[1] </td>
                    <td class="col-lg-6 text-center">@i[2] </td>
                    <td class="col-lg-1 text-center">@i[3] </td>
                    <td class="col-lg-2 text-center">@i[4]</td>
                </tr>
            }
        }
    </tbody>
</table>
